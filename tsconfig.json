{
  "compilerOptions": {
    "target": "ESNEXT",
    "module": "commonjs",
    "removeComments": true,
    "strict": true,
     "moduleResolution": "node",
     "resolveJsonModule": true,
     "typeRoots": [
       "node_modules/@types",
       "types"
     ],
     "types": [
       "vue-eslint-parser",
       "mocha"
     ],
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
