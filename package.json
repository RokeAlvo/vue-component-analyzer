{
  "name": "@tkskto/vue-component-analyzer",
  "version": "0.0.1",
  "description": "Analyze dependency tree for Vue.js SFC (Single File Component)",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "bin": {
    "analyze": "bin/analyze.js"
  },
  "scripts": {
    "dev": "rollup --config -w",
    "build": "rollup --config",
    "lint": "eslint -c .eslintrc.json src",
    "cover": "npm run cover:test && npm run cover:report",
    "cover:test": "nyc npm run test:base -- --timeout 60000",
    "cover:report": "nyc report --reporter=html",
    "test": "nyc npm run test:base -- --timeout 60000",
    "test:base": "mocha --reporter dot"
  },
  "keywords": [],
  "author": {
    "name": "tkskto",
    "url": "https://github.com/tkskto/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tkskto/vue-component-analyzer.git"
  },
  "license": "MIT",
  "files": [
    "dist",
    "bin",
    "types"
  ],
  "dependencies": {
    "commander": "^6.0.0",
    "express": "^4.17.1",
    "vue-eslint-parser": "^7.1.0",
    "ws": "^7.3.1"
  },
  "devDependencies": {
    "@mitsue/eslint-config": "^2.0.0",
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-typescript": "^5.0.2",
    "@types/express": "^4.17.7",
    "@types/ws": "^7.2.6",
    "@typescript-eslint/eslint-plugin": "^3.9.0",
    "@typescript-eslint/parser": "^3.9.0",
    "eslint": "^7.7.0",
    "husky": "4.2.5",
    "lint-staged": "10.2.11",
    "mocha": "^7.1.2",
    "nyc": "^15.1.0",
    "rollup": "^2.26.0",
    "rollup-plugin-license": "^2.2.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "typescript": "^3.9.7"
  },
  "lint-staged": {
    "*.ts": [
      "npm run lint"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
