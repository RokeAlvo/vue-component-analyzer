{
  "name": "@tkskto/vue-component-analyzer",
  "version": "0.0.3",
  "description": "Analyze dependency tree for Vue.js SFC (Single File Component)",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "bin": {
    "vca": "bin/analyze.js"
  },
  "scripts": {
    "dev": "rollup --config -w",
    "build": "rollup --config",
    "lint": "eslint -c .eslintrc.json src",
    "cover": "npm run cover:test && npm run cover:report",
    "cover:test": "nyc npm run test:base -- --timeout 60000",
    "cover:report": "nyc report --reporter=html",
    "test": "nyc npm run test:base -- --timeout 60000",
    "test:base": "mocha --reporter dot"
  },
  "keywords": [],
  "author": {
    "name": "tkskto",
    "url": "https://github.com/tkskto/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tkskto/vue-component-analyzer.git"
  },
  "license": "MIT",
  "files": [
    "dist",
    "bin",
    "types",
    "views"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/",
    "access": "public"
  },
  "dependencies": {
    "commander": "6.1.0",
    "ejs": "3.1.5",
    "express": "4.17.1",
    "globby": "11.0.1",
    "mkdirp": "1.0.4",
    "opener": "1.5.2",
    "vue-eslint-parser": "7.1.0",
    "ws": "7.3.1"
  },
  "devDependencies": {
    "@mitsue/eslint-config": "3.0.0",
    "@rollup/plugin-commonjs": "15.1.0",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-node-resolve": "9.0.0",
    "@rollup/plugin-typescript": "6.0.0",
    "@types/express": "4.17.8",
    "@types/opener": "1.4.0",
    "@types/ws": "7.2.7",
    "@typescript-eslint/eslint-plugin": "4.3.0",
    "@typescript-eslint/parser": "4.3.0",
    "eslint": "7.10.0",
    "husky": "4.3.0",
    "lint-staged": "10.4.0",
    "mocha": "8.1.3",
    "nyc": "15.1.0",
    "rollup": "2.28.2",
    "rollup-plugin-license": "2.2.0",
    "rollup-plugin-node-builtins": "2.1.2",
    "typescript": "4.0.3"
  },
  "lint-staged": {
    "*.ts": [
      "npm run lint"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
